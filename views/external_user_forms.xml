<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  <record model="ir.ui.view" id="arkamen_externaluser_form">
    <field name="name">arkamen.externaluser.form</field>
    <field name="model">arkamen.externaluser</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="full_name"/>
            <field name="contact"/>
            <field name="skills"/>
            <field name="availability"/>
            <field name="tariff"/>
            <field name="cv"/>
            <field name="work_review"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>
  
</odoo>